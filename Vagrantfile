  config.vm.define "windows" do |web|
    web.vm.box = "StefanScherer/windows_2016"
  end
  config.vm.define "windows2" do |web|
    web.vm.box = "jacqinthebox/windowsserver2016"
  end
  config.vm.define "one" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
  config.vm.define "two" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
  config.vm.define "three" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
  config.vm.define "four" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
  config.vm.define "five" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
  config.vm.define "six" do |web|
    web.vm.box = "puppetlabs/centos-7.2-64-nocm"
  end
end
